{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "theme": "Catppuccin Mocha (Blur)",
  "features": {
    "inline_completion_provider": "copilot"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "Zed Plex Mono"
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16
}
